{
    "name": "sobolan/rest-negotiator",
    "license": "MIT",
    "type": "library",
    "description": "A library that allows negotiating content in your REST Webservices",
    "authors": [
        {
            "name": "Radu Murzea",
            "email": "radu.murzea@gmail.com",
            "homepage": "http://radumurzea.net/",
            "role": "Project Manager"
        }
    ],
    "autoload": {
        "psr-4": {
            "SoboLAN\\RestNegotiator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoboLAN\\RestNegotiator\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.9",
        "ext-json": "*",
        "symfony/serializer": "~2.8|~3.0",
        "symfony/yaml": "~2.8|~3.0",
        "symfony/routing": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/http-foundation": "~2.8|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "config": {
        "bin-dir": "bin"
    }
}
